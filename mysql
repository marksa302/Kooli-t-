SELECT *
FROM TAK18_Salundi.yl21_Books
WHERE release_date > 2010 AND type = 'new'
;

select *
from TAK18_Salundi.yl21_Books
where release_date < 1970
and type = 'used'
and price < 20
;

select count(*)
from TAK18_Salundi.yl21_Orders
group by year(order_date)
;

select *
from TAK18_Salundi.yl21_Orders
left join TAK18_Salundi.yl21_Books
;
